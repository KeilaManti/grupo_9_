<!DOCTYPE html>
<html>

	<%- include("../partials/headAdmin") %>
<body>
	
	<%- include("../partials/headerAdmin")%>

	<%- include("../partials/sidebarAdmin") %>
	<main class="col-md-9 ms-sm-auto col-lg-10 px-md-4 admin">
		<h2>Agregar Marca</h2>
        <form class="row g-3" action="/admin/brand/create" method="POST">
            <div class="col-12">
                <label for="inputName" class="form-label">Nueva categoria</label>
                <input type="text" class="form-control" id="inputName" name="brand">
                
            </div>
            <button type="submit" class="btn btn-primary">
                CREAR
            </button>
        </form>
        <h2 class="mt-5">Marcas</h2>
        <table class="table table-striped table-sm ">
            <tbody>
                <% brand.forEach(brands => { %>
                    <tr>
                        <td><i class="fas fa-circle"></i><span class="m-3"><%= brands.brand %></span></td>
                    </tr>
                <% }) %>
            </tbody>
          </table>
	</main>
</body>
</html>